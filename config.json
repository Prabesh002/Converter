            "dithering": true,
            "contrast": 1.5,
            "noise_level": 0.2,
            "color_mode": "monochrome"
        },
        "classic_pixel": {
            "name": "Classic Pixel Art",
            "description": "Clean pixel art with limited palette",
            "pixel_size": 3,
            "dithering": false,
            "contrast": 1.2,
            "noise_level": 0,
            "color_mode": "limited_palette"
        },
        "glitch": {
            "name": "Glitch Art",
            "description": "Distorted pixel art with digital artifacts",
            "pixel_size": 2,
            "dithering": true,
            "contrast": 1.3,
            "noise_level": 0.5,
            "color_mode": "rgb_shift"
        }
    }
}
  